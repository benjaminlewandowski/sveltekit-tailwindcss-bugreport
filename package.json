{
    "name": "sveltekit-bug-report",
    "license": "MIT",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit package",
        "preview": "vite preview",
        "prepare": "svelte-kit sync",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "npm-run-all --parallel lint:*",
        "lint:svelte-check": "svelte-check",
        "lint:stylelint": "stylelint ./src/**/*.{css,postcss,svelte}",
        "lint:eslint": "eslint ./src/**/*.{ts,svelte}",
        "fix": "npm-run-all --sequential fix:*",
        "fix:stylelint": "stylelint ./src/**/*.{css,postcss,svelte} --fix",
        "fix:eslint": "eslint --fix ./src/**/*.{ts,svelte}",
        "fix:prettier": "prettier --write ./src/**/*.{svelte,ts,json,js}"
    },
    "devDependencies": {
        "@ky-is/tailwindcss-plugin-width-height": "1.0.0",
        "@sveltejs/adapter-auto": "1.0.0-next.64",
        "@sveltejs/kit": "1.0.0-next.396",
        "@tailwindcss/aspect-ratio": "0.4.0",
        "@tailwindcss/forms": "0.5.2",
        "@tailwindcss/line-clamp": "0.4.0",
        "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "0.5.4",
        "autoprefixer": "10.4.7",
        "cssnano": "5.1.12",
        "daisyui": "2.20.0",
        "postcss": "8.4.14",
        "postcss-combine-media-query": "1.0.1",
        "postcss-html": "1.5.0",
        "postcss-import": "14.1.0",
        "postcss-load-config": "4.0.1",
        "postcss-preset-env": "7.7.2",
        "postcss-short": "5.0.0",
        "postcss-sort-media-queries": "4.2.1",
        "postcss-sorting": "7.0.1",
        "postcss-utilities": "0.8.4",
        "svelte": "3.49.0",
        "svelte-preprocess": "4.10.7",
        "tailwindcss": "3.1.6",
        "tailwindcss-animatecss": "1.0.7",
        "tailwindcss-fluid": "0.2.0",
        "tailwindcss-logical": "3.0.0",
        "tailwindcss-pseudo-elements": "2.0.0",
        "tailwindcss-textshadow": "2.1.3",
        "tailwindcss-truncate-multiline": "1.0.3",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "vite": "3.0.3"
    },
    "type": "module",
    "typings": "global.d.ts"
}
